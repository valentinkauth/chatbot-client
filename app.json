{
  "expo": {
    "name": "GeMuKi-Chatbot",
    "slug": "chatbot-client",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/gemuki_icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gemuki.chatbot"
    },
    "android": {
      "package": "com.gemuki.chatbot"
    },
    "notification": {
      "icon": "./assets/gemuki_icon_notification.png"
    }
  }
}
